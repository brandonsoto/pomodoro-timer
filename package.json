{
    "name": "pomodoro-timer",
    "displayName": "Pomodoro Timer",
    "description": "Pomodoro Technique support",
    "version": "0.0.1",
    "publisher": "Brandon Soto",
    "repository": {
        "type": "git",
        "url": "https://github.com/brandonsoto/pomodoro-timer"
    },
    "bugs": {
        "url": "https://github.com/brandonsoto/pomodoro-timer/issues"
    },
    "keywords": [
        "pomodoro",
        "timer",
        "productivity"
    ],
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "license": "MIT",
    "contributes": {
        "configurations": {
            "type": "object",
            "title": "Pomodoro",
            "properties": {
                "pomodoro.interval": {
                    "type": "int",
                    "default": "1500000",
                    "description": "The interval length of the timer"
                }
            }
        },
        "commands": [
            {
                "command": "extension.startTimer",
                "title": "Start Timer",
                "category": "Pomodoro"
            },
            {
                "command": "extension.stopTimer",
                "title": "Stop Timer",
                "category": "Pomodoro"
            },
            {
                "command": "extension.showAll",
                "title": "Show all commands",
                "category": "Pomodoro"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}